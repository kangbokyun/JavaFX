            <PasswordField fx:id="signuppwtxt1" layoutX="27.0" layoutY="103.0" prefHeight="34.0" prefWidth="119.0" promptText="UserPw1" />
            <PasswordField fx:id="signuppwtxt2" layoutX="154.0" layoutY="103.0" prefHeight="34.0" prefWidth="119.0" promptText="UserPw2" />
         </children>
      </AnchorPane>
   </children>
