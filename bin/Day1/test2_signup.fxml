            </ImageView>
            <TextField fx:id="signupname" layoutX="27.0" layoutY="144.0" prefHeight="34.0" prefWidth="119.0" promptText="User Name">
               <font>
                  <Font name="Symbol" size="12.0" />
               </font>
            </TextField>
            <TextField fx:id="signupemailtxt" layoutX="154.0" layoutY="144.0" prefHeight="34.0" prefWidth="119.0" promptText="User Email">
