            <TextField fx:id="signupidtxt" layoutX="28.0" layoutY="61.0" prefHeight="34.0" prefWidth="244.0" promptText="User ID">
               <font>
                  <Font name="Symbol" size="12.0" />
               </font>
            </TextField>
            <Button fx:id="btnsignup" alignment="CENTER" blendMode="EXCLUSION" cache="true" contentDisplay="TOP" layoutX="26.0" layoutY="206.0" mnemonicParsing="false" onAction="#signup" prefHeight="43.0" prefWidth="244.0" style="-fx-border-color: #ffffff;" text="회원가입">
