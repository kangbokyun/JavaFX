<AnchorPane fx:id="SignupPane" prefHeight="506.0" prefWidth="545.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.SignupController">
   <children>
      <ImageView fitHeight="700.0" fitWidth="722.0" pickOnBounds="true" preserveRatio="true">
